//  Использование - Булево - начальное значение Истина.
// 
Процедура ПриЗаполненииИспользованияВидаДоступа(ВидДоступа, Использование) Экспорт
	
	УстановитьПривилегированныйРежим(Истина);
	
	Если ВидДоступа = "ГруппыМестХранения" Тогда
        Использование = Константы.ИспользоватьГруппыДоступаМестХранения.Получить();
		
    ИначеЕсли ВидДоступа = "ГруппыКонтрагентов" Тогда
        Использование = Константы.ИспользоватьГруппыДоступаКонтрагентов.Получить();
		
    ИначеЕсли ВидДоступа = "ГруппыКатегорийБазыЗнаний" Тогда
        Использование = Константы.ИспользоватьГруппыДоступаКатегорийБазыЗнаний.Получить();
		
    КонецЕсли;
	
КонецПроцедуры

