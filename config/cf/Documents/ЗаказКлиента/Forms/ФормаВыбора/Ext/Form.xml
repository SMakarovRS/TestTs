				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>Список.Ссылка</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ВЫБОР
		КОГДА НЕ ДокументЗаказКлиента.Проведен
			ТОГДА 4
		ИНАЧЕ ВЫБОР
				КОГДА ЕСТЬNULL(ОплатаДокументовОстатки.СуммаОстаток, 0) &lt;= 0
					ТОГДА 3
				КОГДА ЕСТЬNULL(ОплатаДокументовОстатки.СуммаОстаток, 0) &gt;= ДокументЗаказКлиента.СуммаДокумента
					ТОГДА 0
				КОГДА ЕСТЬNULL(ОплатаДокументовОстатки.СуммаОстаток, 0) &gt; ДокументЗаказКлиента.СуммаДокумента / 2
					ТОГДА 1
				КОГДА ЕСТЬNULL(ОплатаДокументовОстатки.СуммаОстаток, 0) &lt;= ДокументЗаказКлиента.СуммаДокумента / 2
					ТОГДА 2
				ИНАЧЕ 0
			КОНЕЦ
	КОНЕЦ КАК НомерКартинкиОплачено,
	ВЫБОР
		КОГДА НЕ ДокументЗаказКлиента.Проведен
			ТОГДА 4
		ИНАЧЕ ВЫБОР
				КОГДА ЕСТЬNULL(ЗаказыКлиентовОстатки.КоличествоОстаток, 0) &lt;= 0
					ТОГДА 3
				КОГДА ЕСТЬNULL(ЗаказыКлиентовОстатки.КоличествоОстаток, 0) &gt;= ДокументЗаказКлиента.КоличествоВДокументе
					ТОГДА 0
				КОГДА ЕСТЬNULL(ЗаказыКлиентовОстатки.КоличествоОстаток, 0) &gt; ДокументЗаказКлиента.КоличествоВДокументе / 2
					ТОГДА 1
				КОГДА ЕСТЬNULL(ЗаказыКлиентовОстатки.КоличествоОстаток, 0) &lt;= ДокументЗаказКлиента.КоличествоВДокументе / 2
					ТОГДА 2
				ИНАЧЕ 0
			КОНЕЦ
	КОНЕЦ КАК НомерКартинкиОтгружено,
	ВЫБОР
		КОГДА НаличиеФайлов.ЕстьФайлы ЕСТЬ NULL
			ТОГДА 0
		КОГДА НаличиеФайлов.ЕстьФайлы
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК ЕстьФайлы,
	ДокументЗаказКлиента.Ссылка КАК Ссылка,
	ДокументЗаказКлиента.ВерсияДанных КАК ВерсияДанных,
	ДокументЗаказКлиента.ПометкаУдаления КАК ПометкаУдаления,
	ДокументЗаказКлиента.Номер КАК Номер,
	ДокументЗаказКлиента.Дата КАК Дата,
	ДокументЗаказКлиента.Проведен КАК Проведен,
	ДокументЗаказКлиента.Организация КАК Организация,
	ДокументЗаказКлиента.Автор КАК Автор,
	ДокументЗаказКлиента.Контрагент КАК Контрагент,
	ДокументЗаказКлиента.Договор КАК Договор,
	ДокументЗаказКлиента.ДатаПоступления КАК ДатаПоступления,
	ДокументЗаказКлиента.ВалютаДокумента КАК ВалютаДокумента,
	ДокументЗаказКлиента.НалогообложениеНДС КАК НалогообложениеНДС,
	ДокументЗаказКлиента.СуммаВключаетНДС КАК СуммаВключаетНДС,
	ДокументЗаказКлиента.НДСВключатьВСтоимость КАК НДСВключатьВСтоимость,
	ДокументЗаказКлиента.Курс КАК Курс,
	ДокументЗаказКлиента.Кратность КАК Кратность,
	ДокументЗаказКлиента.СуммаДокумента КАК СуммаДокумента,
	ДокументЗаказКлиента.Комментарий КАК Комментарий,
	ДокументЗаказКлиента.СостояниеЗаказа КАК СостояниеЗаказа,
	ДокументЗаказКлиента.ПоложениеДатыПоступления КАК ПоложениеДатыПоступления,
	ДокументЗаказКлиента.Ответственный КАК Ответственный,
	ДокументЗаказКлиента.Бюджет КАК Бюджет,
	ДокументЗаказКлиента.ПериодБюджета КАК ПериодБюджета,
	ДокументЗаказКлиента.СтатьяДоходовРасходов КАК СтатьяДоходовРасходов,
	ДокументЗаказКлиента.ПоложениеСтатьиДоходовРасходов КАК ПоложениеСтатьиДоходовРасходов,
	ДокументЗаказКлиента.МестоХранения КАК МестоХранения,
	ДокументЗаказКлиента.МоментВремени КАК МоментВремени,
	ДокументЗаказКлиента.Номенклатура.(
		Ссылка КАК Ссылка,
		НомерСтроки КАК НомерСтроки,
		Всего КАК Всего,
		ДатаПоступления КАК ДатаПоступления,
		ЕдиницаИзмерения КАК ЕдиницаИзмерения,
		Количество КАК Количество,
		Номенклатура КАК Номенклатура,
		Описание КАК Описание,
		СтавкаНДС КАК СтавкаНДС,
		СтатьяДоходовРасходов КАК СтатьяДоходовРасходов,
		Сумма КАК Сумма,
		СуммаНДС КАК СуммаНДС,
		Цена КАК Цена
	) КАК Номенклатура,
	ВЫБОР
		КОГДА ДокументЗаказКлиента.Закрыт
			ТОГДА &amp;Закрыт
		ИНАЧЕ &amp;КромеЗакрытых
	КОНЕЦ КАК ОтборЗакрыт,
	ДокументЗаказКлиента.Закрыт КАК Закрыт,
	ДокументЗаказКлиента.ДатаВходящегоДокумента КАК ДатаВходящегоДокумента,
	ДокументЗаказКлиента.НомерВходящегоДокумента КАК НомерВходящегоДокумента
ИЗ
	Документ.ЗаказКлиента КАК ДокументЗаказКлиента
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ОплатаДокументов.Остатки КАК ОплатаДокументовОстатки
		ПО ДокументЗаказКлиента.Ссылка = ОплатаДокументовОстатки.Документ
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ЗаказыКлиентов.Остатки КАК ЗаказыКлиентовОстатки
		ПО ДокументЗаказКлиента.Ссылка = ЗаказыКлиентовОстатки.ЗаказКлиента
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.НаличиеФайлов КАК НаличиеФайлов
		ПО ДокументЗаказКлиента.Ссылка = НаличиеФайлов.ОбъектСФайлами</QueryText>
				<MainTable>Document.ЗаказКлиента</MainTable>
