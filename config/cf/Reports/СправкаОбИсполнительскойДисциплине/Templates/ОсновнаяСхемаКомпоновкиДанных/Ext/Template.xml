////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Задание.Ссылка КАК Задание,
	Задание.Организация КАК Организация,
	Задание.Процесс КАК Процесс,
	Задание.Сервис КАК Сервис,
	Задание.Инициатор КАК Инициатор,
	Задание.Тема КАК Тема,
	ЕСТЬNULL(Задание.ТекущийИсполнитель, ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)) КАК Исполнитель,
	СУММА(1) КАК Количество,
	СУММА(ВЫБОР
			КОГДА Задание.Выполнено
						И Задание.КрайняяДатаВыполнения &lt;&gt; ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
						И Задание.КрайняяДатаВыполнения &gt; Задание.ДатаВыполнения
					ИЛИ Задание.Выполнено
						И Задание.КрайняяДатаВыполнения = ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
				ТОГДА 1
			ИНАЧЕ 0
		КОНЕЦ) КАК ВСрок,
	СУММА(ВЫБОР
			КОГДА Задание.Выполнено
					И Задание.КрайняяДатаВыполнения &lt;&gt; ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
					И Задание.КрайняяДатаВыполнения &lt; Задание.ДатаВыполнения
				ТОГДА 1
			ИНАЧЕ 0
		КОНЕЦ) КАК НеВСрок,
	СУММА(ВЫБОР
			КОГДА НЕ Задание.Выполнено
				ТОГДА 1
			ИНАЧЕ 0
		КОНЕЦ) КАК НеВыполнено,
	СУММА(ВЫБОР
			КОГДА Задание.Выполнено
				ТОГДА 1
			ИНАЧЕ 0
		КОНЕЦ) КАК Выполнено,
	СУММА(ВЫБОР
			КОГДА НЕ Задание.Выполнено
				ТОГДА 1
			ИНАЧЕ 0
		КОНЕЦ) КАК Выполняется,
	СУММА(ВЫБОР
			КОГДА Задание.КрайняяДатаВыполнения &lt;&gt; ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)
					И (Задание.Выполнено = ЛОЖЬ
							И Задание.КрайняяДатаВыполнения &lt; &amp;ТекущаяДата
						ИЛИ Задание.Выполнено = ИСТИНА
							И Задание.КрайняяДатаВыполнения &lt; Задание.ДатаВыполнения)
				ТОГДА 1
			ИНАЧЕ 0
		КОНЕЦ) КАК Просрочено,
	Задание.Проект КАК Проект,
	ВТ_Сотрудники.Подразделение КАК Подразделение,
	ВТ_Сотрудники.Сотрудник КАК Сотрудник,
	Задание.Категория КАК Категория,
	Задание.СпособСоздания КАК СпособСоздания,
	Задание.Спринт КАК Спринт,
	Задание.Баллы КАК Баллы,
	ВЫБОР
		КОГДА Задание.Выполнено
			ТОГДА Задание.Баллы
		ИНАЧЕ 0
	КОНЕЦ КАК БаллыВыполнено,
	ВЫБОР
		КОГДА НЕ Задание.Выполнено
			ТОГДА Задание.Баллы
		ИНАЧЕ 0
	КОНЕЦ КАК БаллыНеВыполнено,
	Задание.ТипПроцесса КАК ТипПроцесса,
	Задание.Влияние КАК Влияние,
	Задание.Срочность КАК Срочность
ИЗ
	Документ.Задание КАК Задание
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_Сотрудники КАК ВТ_Сотрудники
		ПО Задание.ТекущийИсполнитель.Сотрудник = ВТ_Сотрудники.Сотрудник
ГДЕ
	Задание.ТекущийИсполнитель &lt;&gt; ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
	И Задание.Дата &gt;= ВЫБОР
			КОГДА &amp;НачалоПериода = НЕОПРЕДЕЛЕНО
				ТОГДА ДАТАВРЕМЯ(1, 1, 1)
			ИНАЧЕ &amp;НачалоПериода
		КОНЕЦ
	И Задание.Дата &lt;= ВЫБОР
			КОГДА &amp;КонецПериода = НЕОПРЕДЕЛЕНО
					ИЛИ &amp;КонецПериода = ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА ДАТАВРЕМЯ(3999, 12, 31)
			ИНАЧЕ &amp;КонецПериода
		КОНЕЦ
	И НЕ Задание.ПометкаУдаления
	И (&amp;ДоступностьРолиПросмотраЧужихЗаданий
			ИЛИ &amp;ДоступностьРолиПолныеПрава
			ИЛИ &amp;ДоступностьРолиПросмотраНеРаспределенныхЗаданий
				И ВЫБОР
					КОГДА Задание.ТекущийИсполнитель.Ссылка ЕСТЬ NULL
						ТОГДА ИСТИНА
					ИНАЧЕ ЛОЖЬ
				КОНЕЦ
			ИЛИ Задание.УчастникиЗадания.Участник В (&amp;СписокПользователейИГруппПользователей))

СГРУППИРОВАТЬ ПО
	Задание.Ссылка,
	Задание.Организация,
	Задание.Процесс,
	Задание.Сервис,
	Задание.Инициатор,
	Задание.Тема,
	Задание.Проект,
	ВТ_Сотрудники.Подразделение,
	ВТ_Сотрудники.Сотрудник,
	ЕСТЬNULL(Задание.ТекущийИсполнитель, ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)),
	Задание.Категория,
	Задание.СпособСоздания,
	Задание.Спринт,
	Задание.Баллы,
	ВЫБОР
		КОГДА Задание.Выполнено
			ТОГДА Задание.Баллы
		ИНАЧЕ 0
	КОНЕЦ,
	ВЫБОР
		КОГДА НЕ Задание.Выполнено
			ТОГДА Задание.Баллы
		ИНАЧЕ 0
	КОНЕЦ,
	Задание.ТипПроцесса,
	Задание.Влияние,
	Задание.Срочность</query>
